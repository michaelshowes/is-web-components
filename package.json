{
  "name": "is-web-components",
  "version": "1.0.2",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/is-web-components/is-web-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/is-web-components/is-web-components.esm.js",
      "require": "./dist/is-web-components/is-web-components.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@stencil/core": "^4.7.0",
    "@stencil/sass": "^3.0.12",
    "@stencil/store": "^2.0.16",
    "@storybook/addon-a11y": "^8.1.9",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@storybook/web-components": "^8.1.9",
    "@storybook/web-components-vite": "^8.1.9",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.1.4",
    "puppeteer": "^21.9.0",
    "storybook": "^8.1.9"
  },
  "license": "MIT",
  "dependencies": {
    "stencil-tailwind-plugin": "^1.8.0",
    "tailwindcss": "^3.4.4"
  }
}
